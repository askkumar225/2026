function formatdate(s){
	var d = new Date(s);
	day = d.getDate();
	mon = d.getMonth()+1;
	year = d.getFullYear();
	day = day<10?"0"+day:day;
	mon = mon<10?"0"+mon:mon;
	var yead = year+"-"+mon+"-"+day;
	return yead;

}

